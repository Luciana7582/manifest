{
    "manifest_version":3,
    "name":"Substituidor de Imagens",
    "version":"1.0" ,
    "permissions":["activeTab","scripting"],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["contentScript.js"]
        }
    ],
    "web_accessible_resources":[
        {
            "resources":["img/image.jpg"],
            "matches":["<all_urls>"]
        }
    ]
}
